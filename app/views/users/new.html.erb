<div class="form">
<h1>Register here! Prepare to be underappreciated!</h1>
  <%= form_for @user do |f| %>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %><br>
  <br>
  <%= f.submit "Sign Up"%><br>
  <% end %>
</div>
